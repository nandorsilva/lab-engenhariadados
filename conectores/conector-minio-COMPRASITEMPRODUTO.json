        "timezone": "America/Sao_Paulo",
        "partition.duration.ms": "3600000",
        "key.converter": "org.apache.kafka.connect.storage.StringConverter",
        "value.converter": "org.apache.kafka.connect.json.JsonConverter",
        "value.converter.schemas.enable": "false",
        "transforms": "ReplaceNulls",
        "transforms.ReplaceNulls.type": "org.apache.kafka.connect.transforms.ReplaceField$Value",
        "transforms.ReplaceNulls.null.replacement": "{}",
        "aws.access.key.id": "cursolab",
        "aws.secret.access.key": "cursolab"

